{
  "name": "@swisstronik/sdk",
  "version": "1.8.0",
  "description": "TypeScript SDK for Swisstronik Network",
  "license": "Apache-2.0",
  "source": "src/index.ts",
  "module": "build/esm/index.js",
  "main": "build/cjs/index.js",
  "types": "build/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "default": "./build/esm/index.js"
    },
    "./*": {
      "types": "./build/types/*.d.ts",
      "import": "./build/esm/*.js",
      "require": "./build/cjs/*.js",
      "default": "./build/esm/*.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --no-cache --runInBand",
    "build": "rm -rf build && npm run build:types && npm run build:esm && npm run build:cjs && npm run fixup",
    "build:types": "tsc -p tsconfig.types.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && rollup -c rollup.config.cjs.js",
    "fixup": "bash ../../tools/fixup.sh",
    "clean": "rm -r build",
    "package": "npm run build && npm pack",
    "addscope": "node ../../tools/packagejson name @swisstronik/sdi-sdk"
  },
  "files": [
    "build/**/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@cosmjs/amino": "^0.31.1",
    "@cosmjs/crypto": "^0.31.1",
    "@cosmjs/encoding": "^0.31.1",
    "@cosmjs/math": "^0.31.1",
    "@cosmjs/proto-signing": "^0.31.1",
    "@cosmjs/stargate": "^0.31.1",
    "@cosmjs/tendermint-rpc": "^0.31.1",
    "@cosmjs/utils": "^0.31.1",
    "@stablelib/ed25519": "^1.0.3",
    "@types/bn.js": "^5.1.1",
    "@types/elliptic": "^6.4.14",
    "@types/keccak": "^3.0.2",
    "@types/secp256k1": "^4.0.3",
    "bn.js": "^5.2.1",
    "cosmjs-types": "0.8.0",
    "did-jwt": "^7.2.7",
    "did-resolver": "^4.1.0",
    "elliptic": "^6.5.4",
    "file-type": "^18.5.0",
    "keccak": "^3.0.3",
    "multiformats": "^12.1.1",
    "protobufjs": "6.11.3",
    "secp256k1": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.4",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.5",
    "@semantic-release/npm": "^10.0.5",
    "@semantic-release/release-notes-generator": "^11.0.7",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/uuid": "^9.0.3",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "long": "^4.0.0",
    "rollup": "^4.12.1",
    "rollup-plugin-multi-input": "^1.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uint8arrays": "^4.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "swisstronik",
    "identity",
    "did"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "gitHead": "f91d5febcbab9def4e50af1e4e0b27f5aa04b532"
}
